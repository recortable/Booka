<% title content.title %>

<div class="row24">
  <div id="browser" class="col-left">
    <%= link_to image_tag('iconos/libro.png'), project %>
    <%= render :partial => 'contents/browser', :locals => {:project => project} %>
  </div>
  <div class="col-center-left">
    <%# content.assets.each do |asset| %>
        <%#= image_tag asset.file.url %>
    <%# end %>
    &nbsp;
  </div>
  <div class="col-center-right">
    <h1><%= content.title %></h1>
    <h2 class="author"><%= content.author %></h2>
    <%= render_body content %>
    &nbsp;
  </div>
</div>
<div class="row24">
  <div class="col-left">&nbsp;</div>
  <div class="col-main">
    comentarios
  </div>
</div>



